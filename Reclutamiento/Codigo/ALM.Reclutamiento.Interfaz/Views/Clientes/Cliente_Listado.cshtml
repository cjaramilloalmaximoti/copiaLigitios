
<div class="divMarcoBusqueda">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6" style="margin-top:5px">
            <div class="col-xs-12 col-sm-4 col-md-4 col-lg-4">
                <label for="lblNombre" id="lblNombre" class="labelColumn tamLetra regularTipografia">Nombre / RFC</label>
            </div>
            <div class="col-xs-12 col-sm-8 col-md-8 col-lg-8">
                <input type="text" id="txtCliente_B" maxlength="64" class="form-control inputColumn inputpe tamLetra bookTipografia" name=" txtnombre", placeholder="Nombre / RFC" />
            </div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6" style="margin-top:5px">
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <label for="cmbEstatus" id="lblEstatusFiltro" class="labelColumn tamLetra regularTipografia">Es Activo</label>
            </div>
            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                <select id="cmbEstatus" class="form-control tamLetra bookTipografia">
                    <option value="-1">Todos</option>
                    <option value="1">Activos</option>
                    <option value="0">Inactivos</option>
                </select>
            </div>
        </div>
    </div>
    <div style="clear:both; text-align:right; width:100%">
        <button type="button" value="Buscar" id="btnBuscar" name="Buscar" class="btnBuscarAgregar">
            <img src="@Url.Content("~/Imagenes/Buscar.png")" class="imagBotones" /> Buscar
        </button>
    </div>
</div>

<div class="divMarcoGrid">
    <div class="divSubMarcoGrid">
        <table class="myTable" id="myTable">
            <thead>
                <tr>
                    <th style="width: 15%;">RFC</th>
                    <th style="width: 30%;">Razón Social</th>
                    <th style="width: 20%;">Nombre Comercial</th>
                    <th style="width: 25%;">Contacto Nombre</th>
                    <th>Es Activo</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

