@model ALM.Empresa.Entidades.EClaseApoyoValidacion
<div class="divMarcoBusqueda">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="margin-top:5px">
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <label for="lblNombre" id="lblNombre" class="labelColumn tamLetra regularTipografia">Análista</label>
            </div>
            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                @Html.DropDownListFor(m => m.IdRequerido, Enumerable.Empty<SelectListItem>(), new { @class = "control-label tamLetra regularTipografia", id = "cmbIdAnalista" })
                @Html.ValidationMessageFor(model => model.IdRequerido, null, new { @class = "text-danger tamLetra bookTipografia" })
            </div>
        </div>
    </div>
    <div style="clear:both; text-align:right; width:100%">
        <hr />
        <button type="button" value="Guardar" id="btnAsignar" name="Guardar" class="btnGuardar pull-right">
            Guardar
        </button>
    </div>
</div>
<div id="containerProgressPopUp" class="containerProgress">
    <div class="contentImageProgress">
        <img alt="Espere ..." src="~/Imagenes/progress.gif" />
    </div>
    <div class="backgroundProgress">
    </div>
</div>
